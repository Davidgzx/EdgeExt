{
    "author": "muyuren",
    "description": "Get information of the active tab.",
    "manifest_version": 2,
    "name": "edgeExt",
    "version": "1.0",
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery-3.3.1.js",
                "keyboard.js",
                "scroll.js",
                "hint.js"
            ],
            "css": [
                "vimEdge.css"
            ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_title": "edgeExt",
        "default_popup": "popUp.html"
    }
}